    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "eslint": "^9.17.0",
